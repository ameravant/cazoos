- content_for(:meta_title) { concat "#{ @cms_config['site_settings']['events_title']}" }

%h1= large_icon('Clock') + ' ' + "Schedule for #{@offering.name}" 
=# yield(:meta_title).strip

.box
  .float-right= render :partial => "/shared/search"
  %span.hmenu
    = icon "Plus", new_admin_event_path
    = link_to "Add a new #{@cms_config['site_settings']['event_title']}", new_admin_offering_event_path
